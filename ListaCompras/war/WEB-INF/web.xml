<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>DeleteItem</servlet-name>
		<servlet-class>com.artigo.android.gae.DeleteItem</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteItem</servlet-name>
		<url-pattern>/deleteItem</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>InsertItem</servlet-name>
		<servlet-class>com.artigo.android.gae.InsertItem</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsertItem</servlet-name>
		<url-pattern>/insertItem</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ListItens</servlet-name>
		<servlet-class>com.artigo.android.gae.ListItens</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListItens</servlet-name>
		<url-pattern>/listItens</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ListItensJson</servlet-name>
		<servlet-class>com.artigo.android.gae.ListItensJson</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListItensJson</servlet-name>
		<url-pattern>/listItensJson</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
